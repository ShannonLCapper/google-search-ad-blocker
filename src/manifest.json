{
    "name": "Google Search Ad Blocker",
    "description" : "Block advertisements on Google search pages",
    "version": "1.0.0",
    "permissions": [
        "https://www.google.com/search"
    ],
    "manifest_version": 2,
    "web_accessible_resources": [],
    "content_scripts": [
      {
        "all_frames": true,
        "js": ["content/index.js"],
        "matches": ["https://www.google.com/search*"],
        "run_at": "document_idle"
      }
    ]
}